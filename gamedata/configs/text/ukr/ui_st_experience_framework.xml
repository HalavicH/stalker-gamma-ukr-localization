    </string>
    <string id="ui_mcm_experience_framework_experience_framework_main_REDUCE_PER_COMP">
        <text>
            % зниження досвіду за кожного напарника
        </text>
    </string>
    <string id="ui_mcm_experience_framework_experience_framework_main_FLOAT_EXPERIENCE_MULT">
